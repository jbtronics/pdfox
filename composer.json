{
  "name": "jbtronics/pdfox",
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^1.4.7",
    "phpstan/phpstan-strict-rules": "^1.5",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Jbtronics\\PDFox\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JBtronics\\PDFox\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
